test_typedef(ctx, parent, typedefs) ::= <<
functions_to_run.emplace_back([]() -> bool
{
    std::cout << std::endl << "======================================== Testing typedef: $typedefs.name$ ========================================" << std::endl;
    bool result = true;

    //Minimal
    $check_alias_type(ctx=ctx, typedefs=typedefs, relation=false, relation_name="minimal", relation_capname="Minimal")$
    $check_alias_related_type(ctx=ctx, typedefs=typedefs, relation=false, relation_name="minimal", relation_capname="Minimal")$

