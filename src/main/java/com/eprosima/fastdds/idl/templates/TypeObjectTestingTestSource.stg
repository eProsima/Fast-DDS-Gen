get_type_identifier(ctx, type, relation, relation_name, var_name) ::= <<
    const TypeIdentifier* $type.name$_$relation_name$_$var_name$ = 
    DomainParticipantFactory::get_instance()->type_object_registry()->get_type_identifier(
        "$if(ctx.GenerateTypesROS2)$$type.ROS2Scopedname$$else$$type.scopedname$$endif$",
        $if(type.primitive)$false$else$$relation$$endif$);
>>


